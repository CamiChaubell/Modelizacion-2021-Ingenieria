        "A = [20,50,30,5,7,1,90,120,110]\n",
        "\n",
        "\n",
        "def ordenar(A):\n",
        "  print(A)\n",
        "  for i in A:\n",
        "    for i in range(0,len(A)-1):\n",
        "      if A[i]>A[i+1]:\n",
        "       Aux = A[i+1]\n",
        "       A[i+1]=A[i]\n",
        "       A[i] = Aux\n",
        "  \n",
        "  return A\n",
        "\n",
        "\n",
        "B = ordenar (A)\n",
        "print(B)\n"
      ],
